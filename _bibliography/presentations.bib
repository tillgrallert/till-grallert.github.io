@misc{JarkasEtAl2022BuildingDigitalHumanities,
  type = {Panel Discussion},
  title = {Building {{Digital Humanities Networks}} \& {{Training}}},
  author = {Jarkas, Najla and Kahale, Elie and Grallert, Till and Sfeir, Maya and Peyssard, Jean-Christophe},
  date = {2022-11-14},
  address = {{Online}},
  eventtitle = {Building Digital Humanities 2022},
  url = {https://web.cvent.com/event/811e389e-78de-46cd-877d-b20b9ae9ed85/websitePage:9539e010-a39c-43ef-98ee-41555cf5c59b?RefId=NOV%2014%20Session%2001},
  langid = {english},
  annotation = {Recording: https://www.youtube.com/watch?v=7jQBakwPcjk},
  abstract = {This roundtable discussion offers a case study of ‚ÄòBuilding DH Networks & Training outside the Global North: Digital Humanities Institute Beirut‚Äô. It pulls together practitioners from a range of career paths and academic disciplines to share their experiences with building and sustaining DH training and networks outside the Global North against the backdrop of their collaboration in organising and running the biennial Digital Humanities Institute Beirut (DHIB, established through the initiative of David Wrisley in 2015). Although Arabic, as the predominant language of the Eastern Mediterranean, has one of the largest communities of speakers with more than 400 million native speakers worldwide and an immensely rich textual heritage, it is also a prime example of under-resourced languages in the digital realm. In addition, societies across the region face challenges from climate breakdown, political turmoil, and prolonged violent conflicts, which all negatively impact their access to (digital) resources and (computational) tools. Moreover, the lack of formal, institutional infrastructures requires a creative, if not ‚Äúhacky‚Äù, approach to the digital turn from funding to technical infrastructures. Sustaining the momentum and expanding the local, regional and international networks fostered by the training institute is particularly challenging in the face of the global Covid-19 pandemic and the financial, political and environmental crises that have recently hit Lebanon and Beirut. Based on the input from the panellists, this round table discussion aims at engaging the audience in a fruitful exchange about the challenges of global DH and possible strategies of how to move forward: which formats and approaches can we imagine for successful and sustainable DH training and community building with the specific affordances available to us?}
}

@unpublished{Burckhardt.etal2022DisruptingDigitalMonolingualism,
  type = {Panel discussion},
  title = {Disrupting {{Digital Monolingualism}}},
  author = {Burckhardt, Daniel and Grallert, Till and Gr√ºndler, Beatrice and Romanov, Maxim and Vertan, Christina, Xenia Kudela},
  date = {2022-09-16},
  url = {https://dot2022.de},
  eventtitle = {Deutscher {{Orientalistentag}} ({{DOT}} 2022)},
  langid = {english},
  venue = {{Berlin}},
  abstract = {Working in digital environments in general and engaging in Digital Humanities projects in particular has gained momentum in the DOT community as in all academic disciplines. The new DH section of the DOT 2022 is a good proof of that. However, researchers who deal with non-Latin scripts in digital environments often face challenges and problems that those who work with Latin script based languages are not aware of. Software is mostly developed in English and in or for the ‚ÄúGlobal North‚Äù, thus excluding possible use cases with non-Latin scripts from their agenda.
Under the title of ‚ÄúDisrupting Digital Monolingualism‚Äù (King‚Äôs College 2020), we want to offer a panel discussion aiming to debate different views and experiences with cultural and technological asymmetries in the panelists research in general and in DH projects in particular. We see digital monolingualism not only as a challenge for conducting research with non-Latin scripts in Digital Humanities projects but also as a barrier to global scholarly exchange in general (e.g. lack of visibility of and access to research/sources in non-Anglophone languages and non-Latin scripts).
Furthermore, through this panel discussion we also aim to strengthen the faculty-library dialogue and collaboration in order to co-design more multilingually enabled knowledge infrastructures in academia in the future. The recently established ‚ÄúMultilingual DH Lab‚Äù at the new Ada Lovelace Center for Digital Humanities at Freie Universit√§t Berlin as well as the foundation of a new working group on multilingual DH in the DHd-association are steps in this direction that we want to expand on. The panel discussion thus aims to provide a critical space to elaborate on the relevant questions of multilingual (non-Latin scripts) Digital Humanities, especially when it comes to advocating multilingually enabled knowledge infrastructures and networking within the DOT community and beyond.}
}

